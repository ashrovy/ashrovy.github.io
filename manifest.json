{
  "manifest_version": 2,
  "name": "Chrome Ashrovy",
  "author": "Ron Ashrovy",
  "version": "2.1",
  "minimum_chrome_version": "36.0",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [ "contextMenus", "audio", "audioCapture", "notifications", "videoCapture", "webview", "tabs", "storage", "notifications", "http://*.facebook.com/", "https://*.facebook.com/", "http://*.messenger.com/", "https://*.messenger.com/" ]

}
